{
  "name": "ezmodel",
  "type": "module",
  "version": "0.2.2",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && vite build",
    "test": "jest --detectOpenHandles",
    "publish:push": "git push",
    "publish:patch": "yarn publish:pre && npm version patch && yarn publish:push",
    "publish:minor": "yarn publish:pre && npm version minor && yarn publish:push",
    "publish:major": "yarn publish:pre && npm version major && yarn publish:push",
    "publish:pre": "yarn test && yarn build"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./react": {
      "import": "./dist/react/index.js"
    }
  },
  "peerDependencies": {
    "@types/react": ">=16.8",
    "react": ">=16.8"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@swc/core": "1.3.100",
    "@swc/jest": "0.2.29",
    "@testing-library/react": "14.1.2",
    "@types/jest": "29.5.10",
    "@types/node": "^20.11.16",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "immer": "10.0.3"
  },
  "dependencies": {
    "@wry/equality": "^0.5.7"
  }
}
